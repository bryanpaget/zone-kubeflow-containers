{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "dependencyDashboard": true,
  "customManagers": [
    {
      "customType": "regex",
      "description": "VSCode extensions from GitHub",
      "fileMatch": ["^images/mid/Dockerfile$"],
      "matchStrings": [
        "code-server\\s+--install-extension\\s+(?<packageName>ms-python\\.python)@(?<currentValue>\\d+\\.\\d+\\.\\d+)"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "microsoft/vscode-python",
      "versioningTemplate": "semver"
    },
    {
      "customType": "regex",
      "description": "Quarto extension",
      "fileMatch": ["^images/mid/Dockerfile$"],
      "matchStrings": [
        "code-server\\s+--install-extension\\s+(?<packageName>quarto\\.quarto)@(?<currentValue>\\d+\\.\\d+\\.\\d+)"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "quarto-dev/quarto-vscode",
      "versioningTemplate": "semver"
    }
  ]
}
