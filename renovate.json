{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "ignoreDeps": ["k8scc01covidacr.azurecr.io/sas4c"],
  "dependencyDashboard": true,
  "customManagers": [
    {
      "customType": "regex",
      "description": "VSCode extensions in code-server Dockerfile (Open VSX)",
      "fileMatch": ["^images/mid/Dockerfile$"],
      "matchStrings": [
        "code-server\\s+--install-extension\\s+(?<packageName>ms-python\\.python)@(?<currentValue>\\d+\\.\\d+\\.\\d+)",
        "code-server\\s+--install-extension\\s+(?<packageName>ms-python\\.debugpy)@(?<currentValue>\\d+\\.\\d+\\.\\d+)",
        "code-server\\s+--install-extension\\s+(?<packageName>REditorSupport\\.r)@(?<currentValue>\\d+\\.\\d+\\.\\d+)",
        "code-server\\s+--install-extension\\s+(?<packageName>ms-ceintl\\.vscode-language-pack-fr)@(?<currentValue>\\d+\\.\\d+\\.\\d+)",
        "code-server\\s+--install-extension\\s+(?<packageName>quarto\\.quarto)@(?<currentValue>\\d+\\.\\d+\\.\\d+)",
        "code-server\\s+--install-extension\\s+(?<packageName>dvirtz\\.parquet-viewer)@(?<currentValue>\\d+\\.\\d+\\.\\d+)",
        "code-server\\s+--install-extension\\s+(?<packageName>redhat\\.vscode-yaml)@(?<currentValue>\\d+\\.\\d+\\.\\d+)",
        "code-server\\s+--install-extension\\s+(?<packageName>ms-vscode\\.azurecli)@(?<currentValue>\\d+\\.\\d+\\.\\d+)",
        "code-server\\s+--install-extension\\s+(?<packageName>mblode\\.pretty-formatter)@(?<currentValue>\\d+\\.\\d+\\.\\d+)"
      ],
      "datasourceTemplate": "open-vsx",
      "versioningTemplate": "semver-coerced"
    }
  ],
  "packageRules": [
    {
      "matchDatasources": ["open-vsx"],
      "enabled": true,
      "rangeStrategy": "bump"
    }
  ]
}
