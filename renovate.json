{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json", 
  "ignoreDeps": ["k8scc01covidacr.azurecr.io/sas4c"],
  "dependencyDashboard": true,
  "customManagers": [
    {
      "customType": "regex",
      "description": "Update VSCode extensions from Open VSX",
      "fileMatch": ["^images/mid/Dockerfile$"],
      "matchStrings": [
        "code-server\\s+--install-extension\\s+(?<packageName>[^\\.@\\s]+\\.[^@\\s]+)@(?<currentValue>\\d+\\.\\d+\\.\\d+)"
      ],
      "datasourceTemplate": "open-vsx",
      "versioningTemplate": "semver-coerced"
    }
  ],
  "packageRules": [
    {
      "matchDatasources": ["open-vsx"],
      "enabled": true,
      "rangeStrategy": "bump"
    },
    {
      "matchPackageNames": ["cpptools-linux"],
      "enabled": false
    }
  ]
}
