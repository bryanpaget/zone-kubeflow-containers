{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json", 
  "ignoreDeps": ["k8scc01covidacr.azurecr.io/sas4c"],
  "dependencyDashboard": true,
  "customManagers": [
    {
      "customType": "regex",
      "description": "Update VSCode extensions installed via code-server",
      "fileMatch": ["^images/mid/Dockerfile$"],
      "matchStrings": [
        "code-server\\s+--install-extension\\s+(?<packageName>[^@\\s]+)@(?<currentValue>[\\w.-]+)"
      ],
      "datasourceTemplate": "open-vsx",
      "versioningTemplate": "semver"
    }
  ],
  "packageRules": [
    {
      "matchDatasources": ["open-vsx"],
      "enabled": true,
      "rangeStrategy": "bump"
    }
  ]
}
