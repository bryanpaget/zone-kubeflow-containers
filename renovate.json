{
  "ignoreDeps": ["k8scc01covidacr.azurecr.io/sas4c"],
  "dependencyDashboard": true,
  "customManagers": [
    {
      "customType": "regex",
      "description": "Update VSCode extensions in Dockerfile",
      "managerFilePatterns": ["Dockerfile"],
      "matchStrings": [
        "code-server --install-extension (?<packageName>[^@\\s]+)@(?<currentValue>[^\\s]+)"
      ],
      "datasourceTemplate": "open-vsx",
      "versioningTemplate": "semver"
    }
  ]
}
